{
  "expo": {
    "name": "DGK_Restaurant",
    "slug": "DGK_Restaurant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dgkrestaurant",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kristyson.DGK_Restaurant"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "1072b9a2-efea-4cd1-be31-89479d3f2e94"
      },

      "EXPO_PUBLIC_API_PROVIDER": "back4app",

      "EXPO_PUBLIC_PARSE_APP_ID": "qCIFTHufhZjBI0CKq3VDRc5OLLX3oH8jcJ2WDXAt",
      "EXPO_PUBLIC_PARSE_REST_KEY": "XnWfYKXWVo8e8t8bGsVuqbApaiSr5uDQaUQ218hm",
      "EXPO_PUBLIC_PARSE_SERVER_URL": "https://parseapi.back4app.com",


      "EXPO_PUBLIC_MENU_COLLECTION": "Menu",
      "EXPO_PUBLIC_TEAM_COLLECTION": "Team",
      "EXPO_PUBLIC_UNIT_COLLECTION": "Unit"
    },

    "runtimeVersion": {
      "policy": "appVersion"
    },

    "updates": {
      "url": "https://u.expo.dev/1072b9a2-efea-4cd1-be31-89479d3f2e94"
    }
  }
}
